{
  "name": "mathematic-back-end",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": "6.11.3"
  },
  "scripts": {
    "start": "DEBUG=planatrip-back-end:* nodemon ./app/index.js --exec babel-node --presets es2015,stage-2",
    "test": "mocha tools/babel.tool.js \"app/test/*.test.js\" --reporter progress",
    "lint": "echo 'Running lint...' && eslint app/",
    "lint:fix": "eslint app/ --fix",
    "prepush": "npm run lint && npm run test",
    "heroku-prebuild": "yarn"
  },
  "dependencies": {
    "body-parser": "~1.17.1",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.3",
    "express": "~4.15.2",
    "mongodb": "^2.2.33",
    "mongoose": "^4.13.1",
    "morgan": "~1.8.1",
    "nodemon": "^1.12.1",
    "pug": "~2.0.0-beta11",
    "serve-favicon": "~2.4.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "eslint": "^4.11.0",
    "husky": "^0.14.3",
    "mocha": "^4.0.1"
  },
  "cacheDirectories": [
    "client/node_modules",
    "server/node_modules"
  ]
}
